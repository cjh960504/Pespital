}
</script>
</head>
<body>

<h1 style="color:tomato; text-align:center">PETSPITAL</h1>

<table class="table table-hover">
  <thead>
  <tr>
    <th>이름</th>
    <th>증상</th>
    <th>예방법</th>
  </tr>
</thead>
  
<tbody>
  <%for(var i=0;i<diseaseArray.length;i++){%>
    <%var disease = diseaseArray[i]%>
    <tr class="table-info">
    <input type="hidden" name="disease_id" value="<%=disease.disease_id%>"/>
    <input type="hidden" name="animal_id" value="<%=animal.disease_id%>"/>
    <input type="hidden" name="div_id" value="<%=div.disease_id%>"/>
    <th scope="row"><a href="/disease/detail?disease_id=<%=disease.disease_id%>"><%=disease.disease_name%></a></th>
    <td><%=disease.disease_condition%></td>
    <td><%=disease.disease_preventing%></td>
  </tr>
  <%}%>
</tbody>

  <tr>
      <td colspan="5">
        <button type="button" class="btn btn-outline-primary">글쓰기</button>
      </td>
  </tr>
</table>

